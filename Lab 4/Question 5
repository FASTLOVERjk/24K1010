#include<stdio.h>

int main() {
char c; 
printf("Enter a Character: ");
scanf("%c" , &c);
if (c >= 'a' && c <= 'z') {
printf("%c is a smaller alphabet.\n", c);
}
else if (c >= 'A' && c <= 'Z') {
printf("%c letter is a capital alphabet.\n", c);
}
 else if (c >= '0' && c <= '9') {
printf("%c is a digit.\n", c);
}
else {
printf("%c is a special character.\n", c);
}
return 0;
}
